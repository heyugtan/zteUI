<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    
    <!-- 评分星星样式 -->
    <style name="ratingbar_style" parent="@android:style/Widget.RatingBar">  
       <item name="android:progressDrawable">@drawable/grade_rating_bar</item>  
       <item name="android:numStars">5</item>
       <item name="android:rating">5</item>
       <item name="android:minHeight">32dp</item>  
       <item name="android:maxHeight">28dp</item>  
   </style>
   
    <!-- 评分星星样式  只显示 小 -->
    <style name="ratingbar_style_not_touch" parent="@android:style/Widget.RatingBar">  
       <item name="android:progressDrawable">@drawable/grade_rating_bar_small</item>  
       <item name="android:numStars">5</item>
       <item name="android:rating">4.5</item>
       <item name="android:minHeight">10dp</item>  
       <item name="android:maxHeight">10dp</item>  
       <item name="android:maxWidth">10dp</item>
       <item name="android:minWidth">10dp</item>
   </style>
    

</resources>
